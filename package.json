{
  "name": "projectext",
  "version": "1.0.0",
  "description": "This project is to make sure no scattered files of different extensions remain present",
  "main": "main.js",
  "scripts": {
    "build": "npx @tailwindcss/cli -i ./src/input.css -o ./output.css --content \"./**/*.{html,ejs}\"",
    "watch": "npx @tailwindcss/cli -i ./src/input.css -o ./output.css --content \"./**/*.{html,ejs}\" --watch",
    "start": "node main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
